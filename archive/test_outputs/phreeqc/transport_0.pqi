DATABASE C:\Program Files\USGS\phreeqc-3.8.6-17100-x64\database\phreeqc.dat
TITLE Simple Transport Test - Na=0 mg/L

SOLUTION 0  # Feed water
    units     mg/L
    temp      25
    pH        7.5
    Ca        180
    Mg        80
    Na        0
    Cl        350.0 charge
    C(4)      300 as HCO3

SOLUTION 1-5  # Initial Na-form resin
    units     mg/L
    temp      25
    pH        7.0
    Na        1000
    Cl        1540 charge
    water     0.08 L

EXCHANGE 1-5
    X         0.1  # mol/kg water
    -equilibrate solution 1-5

TRANSPORT
    -cells    5
    -shifts   625
    -flow_direction forward
    -boundary_conditions flux flux
    -punch_frequency 50  # Every 10 BV
    -punch_cells 5

SELECTED_OUTPUT 1
    -file transport_0.sel
    -reset false
    -step true
    -totals Ca Mg Na

USER_PUNCH 1
    -headings Shift BV Ca_mg_L Ca_pct
    -start
    10 PUNCH STEP_NO
    20 BV = STEP_NO * 0.08
    30 PUNCH BV
    40 ca_mg = TOT("Ca") * 40.078 * 1000
    50 PUNCH ca_mg
    60 PUNCH ca_mg / 180.0 * 100
    -end

END
